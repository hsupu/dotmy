{
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools",
        "editor.semanticHighlighting.enabled": true,
        "editor.suggest.insertMode": "replace",
        "editor.wordBasedSuggestions": "off"
    },
    "[json]": {
        "editor.quickSuggestions": {
            "strings": true
        },
        "editor.suggest.insertMode": "replace",
        "editor.tabSize": 2
    },
    "[python]": {
        "diffEditor.ignoreTrimWhitespace": false,
        "editor.formatOnType": false
    },
    "[xml]": {
        "editor.defaultFormatter": "DotJoshJohnson.xml",
        "editor.tabSize": 2
    },
    "[yaml]": {
        "editor.tabSize": 2
    },
    "accessibility.signals.terminalBell": {
        "sound": "on"
    },
    "breadcrumbs.icons": false,
    // "breadcrumbs.showVariables": false,
    // "breadcrumbs.showStrings": false,
    // "breadcrumbs.showOperators": false,
    // "breadcrumbs.showNull": false,
    // "breadcrumbs.showFields": false,
    // "breadcrumbs.showEvents": false,
    // "breadcrumbs.showEnums": false,
    // "breadcrumbs.showEnumMembers": false,
    // "breadcrumbs.showConstructors": false,
    // "breadcrumbs.showConstants": false,
    // "breadcrumbs.showBooleans": false,
    // "breadcrumbs.showProperties": false,
    // "breadcrumbs.showTypeParameters": false,
    "csharp.maxProjectFileCountForDiagnosticAnalysis": 10000,
    "diffEditor.ignoreTrimWhitespace": false,
    "dotnet.codeLens.enableReferencesCodeLens": false,
    "editor.fontFamily": "'Iosevka PU-5', 'Sarasa Fixed SC'",
    "editor.fontLigatures": false,
    "editor.formatOnPaste": false,
    "editor.formatOnSave": false,
    "editor.inlayHints.enabled": "onUnlessPressed",
    "editor.inlineSuggest.enabled": true,
    "editor.roundedSelection": false,
    "editor.rulers": [
        120,
        140
    ],
    "editor.suggestSelection": "first",
    "editor.tokenColorCustomizations": {
        "[Default Light+]": {
            "textMateRules": [
                {
                    "scope": "keyword.operator",
                    "settings": {
                        "foreground": "#D30102"
                    }
                },
                {
                    "scope": [
                        "comment.block.documentation",
                        "meta.tag"
                    ],
                    "settings": {
                        "foreground": "#008000"
                    }
                }
            ]
        }
    },
    "editor.unicodeHighlight.includeComments": false,
    "editor.unicodeHighlight.includeStrings": false,
    "editor.wordSeparators": "`~!@#$%^&*()-=+[]{}\\|:;'\",.<>/? ·！￥…（）【】、：；‘’“”，。《》？　｀～＠＃＄％＾＆＊－＝＋［］｛｝｜＇＂．＜＞／— │",
    "editor.wordWrapColumn": 140,
    "editor.wrappingIndent": "none",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "extensions.experimental.affinity": {
        "eamodio.gitlens": 2,
        "jeff-hykin.better-cpp-syntax": 1,
        "ms-vscode.cpptools": 1
    },
    "files.autoGuessEncoding": true,
    "files.exclude": {},
    "files.hotExit": "onExitAndWindowClose",
    "files.watcherExclude": {},
    "git.autoRepositoryDetection": false,
    "git.countBadge": "tracked",
    "git.mergeEditor": false,
    "git.path": [
        "C:\\Program Files\\Git\\cmd\\git.exe",
        "%LOCALAPPDATA%\\Programs\\Git\\cmd\\git.exe",
        "C:\\opt\\scoop\\shims\\git.exe",
        "C:\\my\\local\\opt\\scoop\\shims\\git.exe"
    ],
    "git.rebaseWhenSync": true,
    "git.repositoryScanMaxDepth": 0,
    "git.smartCommitChanges": "tracked",
    "github.copilot.advanced": {},
    "github.copilot.enable": {
        "*": true,
        "c": true,
        "cpp": true,
        "csharp": true,
        "markdown": true,
        "plaintext": false,
        "python": true,
        "scminput": false,
        "typescript": true,
        "vue": true,
        "yaml": false
    },
    "gitlens.codeLens.authors.enabled": false,
    "gitlens.codeLens.recentChange.enabled": false,
    "gitlens.views.commits.avatars": false,
    "hexeditor.columnWidth": 16,
    "hexeditor.defaultEndianness": "little",
    "hexeditor.inspectorType": "aside",
    "hexeditor.showDecodedText": true,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "powershell.codeFormatting.trimWhitespaceAroundPipe": true,
    "powershell.codeFormatting.useCorrectCasing": true,
    "powershell.integratedConsole.showOnStartup": false,
    "powershell.integratedConsole.suppressStartupBanner": true,
    "powershell.pester.useLegacyCodeLens": false,
    "powershell.powerShellDefaultVersion": "PowerShell (x64)",
    "powershell.promptToUpdatePowerShell": false,
    "powershell.startAsLoginShell.linux": true,
    "remote.SSH.remotePlatform": {
        "bwh1": "linux"
    },
    "search.exclude": {},
