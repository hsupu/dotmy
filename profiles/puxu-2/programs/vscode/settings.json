    "terminal.integrated.enableVisualBell": true,
    "terminal.integrated.fontFamily": "'Iosevka PU-5', 'Sarasa Mono SC'",
    "terminal.integrated.profiles.windows": {
        "Command Prompt": {
            "icon": "terminal-cmd",
