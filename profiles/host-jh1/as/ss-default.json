{
  // listen on :: for dual stack support, no need add [] around.
 "servers": [
      {
          "server": "::",
          "server_port": 40000,
          "method": "chacha20-ietf-poly1305",
          "password": "HelloWorld.",
          "mode": "tcp_and_udp",
          "timeout": 7200,
      },
      {
          "server": "::",
          "server_port": 60000,
          "method": "aes-256-gcm",
          "password": "HelloWorld.",
          //"plugin": "v2ray-plugin",
          //"plugin_opts": "mode=quic;host=github.com",
          "mode": "tcp_and_udp",
          "timeout": 7200,
      },
  ],

  "ipv6_first": false,
  "ipv6_only": false,

  "no_delay": false,
  "keep_alive": 15,

  "udp_timeout": 300,
  "udp_max_asociations": 512,

  "dns": "google"
}
